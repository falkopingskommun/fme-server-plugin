    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.2.3",
    "compression-webpack-plugin": "^6.1.1",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^8.0.0",
